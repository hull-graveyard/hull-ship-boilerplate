{
  "parser": "babel-eslint",
  "ecmaFeatures": {
      "modules": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "key-spacing": [0],
    "quotes": [2, "double"],
    "no-multi-spaces": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0]
  }
}
